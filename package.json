{
  "name": "nester",
  "version": "0.0.1",
  "description": "This project is a fantastic starting point for anyone looking to kick off a new project using **NestJS**. It comes with some pre-written code to help you get started quickly, and it includes features for authentication, notifications, attachments, and user management.",
  "author": "muhammadzadeh",
  "private": false,
  "license": "UNLICENSED",
  "engines": {
    "node": "v22.5.1",
    "npm": "v10.8.2"
  },
  "jest": {
    "preset": "ts-jest",
    "rootDir": "src",
    "coverageDirectory": "../coverage",
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "coverageReporters": [
      "text",
      "text-summary",
      "json"
    ],
    "testEnvironment": "node",
    "globalSetup": "./common/test-pre.ts",
    "verbose": false,
    "setupFiles": [
      "./common/test-setup.ts"
    ]
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "typeorm-ts-node-commonjs"
  },
