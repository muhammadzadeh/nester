{
  "name": "@repo/config",
  "description": "The App config",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "npm build --watch",
    "build": "tsc -b -v",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\""
  },
  "files": [
    "./dist/**"
  ],
  "publishConfig": {
    "access": "public"
  },
  "typesVersions": {
    "*": {
      "*": [
        "src/*"
      ]
    }
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./*": {
      "import": "./dist/*.js",
      "require": "./dist/*.js"
    }
  },
  "dependencies": {
    "@nestjs/mapped-types": "*",
    "@repo/decorator": "*",
    "@repo/validator": "*",
    "@repo/utils": "*",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "node-vault": "^0.10.2"
  },
  "devDependencies": {
    "@repo/eslint-config": "*",
    "@repo/typescript-config": "*",
    "@types/node": "^20.3.1",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "typescript": "5.4.5"
  }
}
